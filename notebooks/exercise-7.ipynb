   "source": [
    "set.seed(983)\n",
    "all_avgs <- c()\n",
    "for (i in 1:3220) {\n",
    "  sample <- rbinom(140, 2, 0.97)\n",
    "  avg <- mean(sample)\n",
    "  all_avgs <- c(all_avgs, avg)\n",
    "}\n",
    "all_avgs_avg <- mean(all_avgs)\n",
    "vesp = 2 * 0.97\n",
    "abs(all_avgs_avg - vesp)"
   ]
